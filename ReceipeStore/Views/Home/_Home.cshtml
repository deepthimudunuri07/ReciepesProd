<!DOCTYPE html>
<html ng-app ="Runapp" >
    <head>
        @*<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />*@
        <title ng-bind="models.helloAngular"></title>
    </head>
<body>
    <div ng-controller="landingPageController">
        <input type="text" ng-model="models.helloAngular" />
        <h1>{{models.helloAngular}}</h1>
        {{"I am " +"you"}}
        {{4 + 10}}

        <input type="text" ng-model="models.anothermodel" />
        <h2>{{models.anothermodel}}</h2>
    </div>
    
    <div ng-controller ="secondControl">
        
        
        <input type="checkbox" ng-model ="models.checkModel" checked = "{{models.checkModel}}" />
        

    </div>
    @*<script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular.min.js"></script>*@
    <script src="@Url.Content("~/Scripts/angular.min.js")" type="text/javascript"></script>
   
    @Scripts.Render("~/bundles/Runapp");

</body>

</html>